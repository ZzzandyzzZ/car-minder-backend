{
  "name": "car-minder-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/main.js",
    "run:migrations": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/db/index.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register src/main.ts"
  },
  "dependencies": {
    "dotenv": "^16.1.3",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@types/pg": "^8.10.1",
    "ts-alias": "^0.0.7",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
